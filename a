	Name = "Auto-Craft:"
})

getgenv().WantedEvoItems = {}
local multiitemevo = sections.MainSection19:Dropdown({
	Name = "Select To Craft [Multi]:",
	Search = true,
	Multi = true,
	Required = false,
	Options = items,
	Default = {"None"},
	Callback = function(Value)
