    Name = "Select Multi Specific Rangers:",
    Search = true,
    Multi = true,
    Required = false,
    Options = rangerStages,
    Default = {"None"},
    Callback = function(Value)
        getgenv().WantedRangers = {}
        for i in pairs(Value) do
